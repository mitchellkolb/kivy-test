<MenuScreen>:
	Button:
		text:"Main"
		on_press:root.manager.current='main'

<MainScreen>:
	Button:
		text:"Menu"
		on_press:root.manager.current='menu'

myScreenManager:
	id: screen_manager
	menu_screen: menu_screen
	main_screen: menu_screen
	
	MenuScreen:
		id: menu_screen
		name: 'menu'
		manager: screen_manager
	MainScreen:
		id: main_screen
		name: 'main'
		manager: screen_manager